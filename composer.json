{
    "name": "goodcat/laravel-l10n",
    "description": "An opinionated Laravel package for app localization",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Goodcat\\L10n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Goodcat\\L10n\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Matteo Palazzo",
            "email": "outlaw.plz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
              "Goodcat\\L10n\\L10nServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "l10n",
        "localization",
        "goodcat"
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.28.1",
        "illuminate/routing": "^12.28.1",
        "illuminate/http": "^12.28.1",
        "tightenco/ziggy": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^10.4",
        "laravel/pint": "^1.22",
        "larastan/larastan": "^3.5",
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
